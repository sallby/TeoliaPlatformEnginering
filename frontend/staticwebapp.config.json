{
    "routes": [
        {
            "route": "/formulaire.html",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 404
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/aad"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/vendor/*",
            "/assets/img/*.{png,jpg,gif}",
            "/assets/css/*",
            "/assets/js/*"
        ]
    }
}