{
    "routes": [
        {
            "route": "/formulaire.html/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/.auth/login/aad",
            "statusCode": 404
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css,scss,js,png,gif,ico,jpg,svg}"
        ]
    }
}